BEGIN 
dbms_scheduler.create_job('"JOB_CONTABILIZAR_NC_ANUL"',
job_type=>'PLSQL_BLOCK', job_action=>
'DECLARE
  PV_CODEMPRESA VARCHAR2(2); 
  PV_PREFIJO VARCHAR2(5);
  PV_CODIGOTIPODOCUMENTO VARCHAR2(4);
  PV_CODIGOTIPODOCFACP VARCHAR2(4);
  PV_CODIGOTIPODOCNC VARCHAR2(4);
  PV_TIPOPROCESO VARCHAR2(200);
  PN_IDDOCUMENTO NUMBER;
  PV_FE_PROCESAR VARCHAR2(200);
  PV_MSNERROR VARCHAR2(3000);
BEGIN
  PV_CODEMPRESA          := ''10'';
  PV_PREFIJO             := ''TN'';
  PV_CODIGOTIPODOCUMENTO := '' '';
  PV_CODIGOTIPODOCFACP   := '' '';
  PV_CODIGOTIPODOCNC     := ''NC'';
  PV_TIPOPROCESO         := ''ANULACION-FAC'';
  PN_IDDOCUMENTO         := NULL;
  PV_FE_PROCESAR         := NULL;
  PV_MSNERROR            := NULL;
  DB_FINANCIERO.FNKG_CONTABILIZAR_FACT_NC.P_REG_MOTIV_FACT_ANULADA(
    PV_PREFIJOEMPRESA      => PV_PREFIJO,
    PV_CODIGOTIPODOCFAC    => PV_CODIGOTIPODOCUMENTO,
