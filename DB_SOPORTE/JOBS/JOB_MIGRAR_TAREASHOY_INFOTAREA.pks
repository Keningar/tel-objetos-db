job_type=>'PLSQL_BLOCK', job_action=>
'DECLARE
			  	PV_MENSAJE_RESPUESTA VARCHAR2(200);
                                PN_DIAS_ACTUALIZA NUMBER;
			BEGIN
                          PN_DIAS_ACTUALIZA := 6;
			  DB_SOPORTE.SPKG_INFO_TAREA.P_SYNC_TAREAS_NUEVAS_DIA(
			    PV_MENSAJE_RESPUESTA => PV_MENSAJE_RESPUESTA
			  );
