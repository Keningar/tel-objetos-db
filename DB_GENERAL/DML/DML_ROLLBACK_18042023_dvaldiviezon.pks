DELETE FROM DB_GENERAL.ADMI_PARAMETRO_DET 
WHERE PARAMETRO_ID = 
(
    SELECT ID_PARAMETRO
    FROM DB_GENERAL.ADMI_PARAMETRO_CAB
    WHERE NOMBRE_PARAMETRO = 'JURISDICCIONES_PE'
)
AND USR_CREACION = 'dvaldiviezon';

DELETE FROM DB_GENERAL.ADMI_PARAMETRO_CAB 
WHERE ID_PARAMETRO = 
(
    SELECT ID_PARAMETRO
    FROM DB_GENERAL.ADMI_PARAMETRO_CAB
    WHERE NOMBRE_PARAMETRO = 'JURISDICCIONES_PE'
)
AND USR_CREACION = 'dvaldiviezon';

COMMIT;
